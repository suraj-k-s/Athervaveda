                    center: {
                        lat: 40.645037,
                        lng: -73.880224
                    },
                });

                var image = '../Assets/Template/Main/images/maps-and-flags.png';
                var beachMarker = new google.maps.Marker({
                    position: {
                        lat: 40.645037,
                        lng: -73.880224
                    },
                    map: map,
                    icon: image
                });
            }
        </script>
